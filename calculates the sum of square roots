#calculates the sum of square roots from 1 to 99, separated by odd and even numbers
import math
# Initialize sums for odd and even numbers
sum_odd = 0
sum_even = 0
# Loop through numbers from 1 to 99
for i in range(1, 100):
    # Check if number is odd or even
    if i % 2 == 0:
        # Add square root to even sum
        sum_even += math.sqrt(i)
    else:
        # Add square root to odd sum
        sum_odd += math.sqrt(i)
# Print results
print("Sum of square roots of even numbers:", sum_even)
print("Sum of square roots of odd numbers:", sum_odd)
